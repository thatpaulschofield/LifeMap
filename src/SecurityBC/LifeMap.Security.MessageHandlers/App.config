<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="MsmqTransportConfig" type="NServiceBus.Config.MsmqTransportConfig, NServiceBus.Core"/>
    <section name="UnicastBusConfig" type="NServiceBus.Config.UnicastBusConfig, NServiceBus.Core"/>
  </configSections>
  <connectionStrings>
    <add name="SecurityEventStore" connectionString="Url = http://localhost:8080/databases/Security"/>
  </connectionStrings>
  <MsmqTransportConfig
                       InputQueue="securityEndpoint"
                       ErrorQueue="securityEndpointError"
                       NumberOfWorkerThreads="1"
                       MaxRetries="5"/>
  <UnicastBusConfig>
    <!--DistributorControlAddress="" DistributorDataAddress="" ForwardReceivedMessagesTo="">-->
    <MessageEndpointMappings>
      <add Messages="LifeMap.Security.Commands" Endpoint="securityEndpoint" />
      <add Messages="LifeMap.Security.Events" Endpoint="securityEndpoint" />
    </MessageEndpointMappings>
  </UnicastBusConfig>
  
  <appSettings>
    <add key="EndpointConfigurationType" value="LifeMap.Security.MessageHandlers.EndpointConfig, LifeMap.Security.MessageHandlers"/>
  </appSettings>
</configuration>