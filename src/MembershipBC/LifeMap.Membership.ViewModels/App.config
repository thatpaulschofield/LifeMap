<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="MsmqTransportConfig" type="NServiceBus.Config.MsmqTransportConfig, NServiceBus.Core"/>
    <section name="UnicastBusConfig" type="NServiceBus.Config.UnicastBusConfig, NServiceBus.Core"/>
  </configSections>
  <connectionStrings>
    <add name="MembershipViewModels" connectionString="Url = http://localhost:8080/databases/MembershipViewModels"/>
  </connectionStrings>
  <MsmqTransportConfig
                       InputQueue="membershipViewModelEndpoint"
                       ErrorQueue="membershipViewModelEndpointError"
                       NumberOfWorkerThreads="1"
                       MaxRetries="5"/>
  <UnicastBusConfig>
    <!--DistributorControlAddress="" DistributorDataAddress="" ForwardReceivedMessagesTo="">-->
    <MessageEndpointMappings>
      <add Messages="LifeMap.Membership.Events" Endpoint="membershipViewModelEndpoint" />
    </MessageEndpointMappings>
  </UnicastBusConfig>
  
  <appSettings>
    <add key="EndpointConfigurationType" value="LifeMap.Membership.ViewModels.EndpointConfig, LifeMap.Membership.ViewModels"/>
  </appSettings>
</configuration>