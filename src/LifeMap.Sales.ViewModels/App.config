<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="MsmqTransportConfig" type="NServiceBus.Config.MsmqTransportConfig, NServiceBus.Core"/>
    <section name="UnicastBusConfig" type="NServiceBus.Config.UnicastBusConfig, NServiceBus.Core"/>
  </configSections>
  <connectionStrings>
    <add name="SalesViewModels" connectionString="Url = http://localhost:8080/databases/SalesViewModels"/>
  </connectionStrings>
  <MsmqTransportConfig
                       InputQueue="salesViewModelEndpoint"
                       ErrorQueue="salesViewModelError"
                       NumberOfWorkerThreads="1"
                       MaxRetries="5"/>
  <UnicastBusConfig>
    <!--DistributorControlAddress="" DistributorDataAddress="" ForwardReceivedMessagesTo="">-->
    <MessageEndpointMappings>
      <add Messages="LifeMap.Sales.Events" Endpoint="salesViewModelEndpoint" />
    </MessageEndpointMappings>
  </UnicastBusConfig>
  
  <appSettings>
    <add key="EndpointConfigurationType" value="LifeMap.Sales.ViewModels.EndpointConfig, LifeMap.Sales.ViewModels"/>
  </appSettings>
</configuration>